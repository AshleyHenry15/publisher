    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch UI",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/publishing-client/main.go",
            "args": [
                "publish-ui",
                "-n",
                "devpwpg",
                "${workspaceFolder}/notebook1"
            ],
        },
        {
            "name": "Launch CLI",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/publishing-client/main.go",
            "args": [
                "publish",
                "-n",
                "badauth2",
                "${workspaceFolder}/notebook1",
                "--debug",
            ],
        }
    ]
}
